        <BeanShellAssertion guiclass="BeanShellAssertionGui" testclass="BeanShellAssertion" testname="BeanShell Assertion">
          <stringProp name="BeanShellAssertion.query">long expectedMinResponseTime = 100;
long expectedMaxResponseTime = 800;
String expectedStatusCode = &quot;200&quot;;

long responseTime = SampleResult.getTime();

boolean responseTimeWithinRange = (responseTime &gt; expectedMinResponseTime &amp;&amp; 
responseTime &lt; expectedMaxResponseTime);

boolean statusCodeAsExpected = SampleResult.getResponseCode().equals(expectedStatusCode);

if (!responseTimeWithinRange) {
    SampleResult.setSuccessful(false);
    SampleResult.setResponseMessage(&quot;Response time not within the expected range: &quot; + responseTime + &quot; ms.&quot;);
}

if (!statusCodeAsExpected) {
    SampleResult.setSuccessful(false);
    SampleResult.setResponseMessage(&quot;Status code is not as expected: &quot; + SampleResult.getResponseCode());
}


if (responseTimeWithinRange &amp;&amp; statusCodeAsExpected) {
    SampleResult.setResponseMessage(&quot;Success: Response time and status code within expected ranges.&quot;);
} 
else {
    SampleResult.setResponseMessage(&quot;Failure: Response time or status code not within expected ranges.&quot;);
}
</stringProp>
